{
  "extends": ["airbnb"],
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
  },
  "rules": {
    "comma-dangle": ["error", "always-multiline", { "functions": "never" }],
    "no-param-reassign": ["error", { "props": false }],
    "import/extensions": false
  }
}
